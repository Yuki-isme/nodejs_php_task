<div class="col-12">
    <div class="card">
        <div class="card-header flex flex-wrap justify-end">
            <h2 class="card-title w-full">List <%- data.module %></h2>

            <div class="clearfix"></div>

            <% if(data.module == 'bill') { %>

                <div class="button-action">
                    <a href="<%- url %>bill/create" class="btn btn-outline-success">
                        Add New Bill
                    </a>
                </div>
                <div class="button-action">
                    <button class="btn btn-outline-success" id="<%- data.module %>_archive" onclick="listJs.archiveConversion()" disabled>
                        Archive
                    </button>
                </div>
                <div class="button-action">
                    <a href="<%- url %>archive" class="btn btn-outline-success">
                        View Archived Items
                    </a>
                </div>
            <% } else { %>
                <div class="button-action">
                    <button class="btn btn-outline-success" id="<%- data.module %>_archive" onclick="listJs.archiveConversion()" disabled>
                        UnArchive
                    </button>
                </div>
                 <div class="button-action">
                    <a href="bill" class="btn btn-outline-success">
                        View Bill Items
                    </a>
                </div>
            <% } %>

            <div class="button-action">
                <form action="/refreshToken" method="post">
                    <button class="btn btn-outline-danger" type="submit">Refresh token</button>
                </form>
            </div>
            <div class="button-action">
                <form action="/logout" method="post">
                    <button class="btn btn-outline-danger" type="submit">Logout</button>
                </form>
            </div>

            <div class="clearfix"></div>
        </div>
        <div>
            <div class="card-body">
                <div class="flex justify-between">
                    <div id="<%- data.module %>_select">
                        <select multiple>
                            <option value="select_all">All</option>
                        </select>
                    </div>
                    <div class="button-action">
                        <button class="btn btn-outline-danger" id="<%- data.module %>_delete" data-value="<%- data.module %>_delete" disabled>
                            Delete
                        </button>
                    </div>
                </div>
                <table class="table table-hover table-bordered" id="<%- data.module %>_tbl" style="width:100%">
                    <thead></thead>
                    <tfoot>
                        <tr>

                        </tr>
                    </tfoot>
                    <tbody></tbody>
                </table>
            </div>

        </div>
    </div>
</div>
