<div class="col-12">
    <div class="card">
        <div class="card-header flex flex-wrap justify-end">
            <h2 class="card-title w-full"><%- module %></h2>

            <div class="clearfix"></div>

<!--            <div class="button-action">-->
<!--                <form action="/refreshToken" method="post">-->
<!--                    <button class="btn btn-outline-danger" type="submit">Refresh token</button>-->
<!--                </form>-->
<!--            </div>-->
<!--            <div class="button-action">-->
<!--                <form action="/logout" method="post">-->
<!--                    <button class="btn btn-outline-danger" type="submit">Logout</button>-->
<!--                </form>-->
<!--            </div>-->

            <div class="clearfix"></div>
        </div>
        <div class="card-header flex flex-wrap">
            <div class="clearfix"></div>
            <% data.items.forEach((item, index) => { %>
                <div class="<%- item?.classCss %>">
                    <% if(typeof item.items !== 'undefined') { %>
                        <% for (const [key, value] of Object.entries(item.items)) { %>
                            <%- value.content %>
                        <% }; %>
                    <% } %>
                </div>
            <% }); %>

            <div class="clearfix"></div>
        </div>
        <div>
            <div class="card-body">
                <table class="table table-hover table-bordered" id="<%- data.module %>_tbl" style="width:100%">
                    <thead></thead>
                    <tfoot>
                    <tr>

                    </tr>
                    </tfoot>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
